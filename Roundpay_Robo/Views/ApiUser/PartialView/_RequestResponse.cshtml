@using Roundpay_Robo.Models
@model LapuReqRes

@if (Model != null)
{
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
    <div class="form-group">
        <label for="Request" class="col-form-label">Request</label>
        <textarea disabled="disabled" class="form-control" style="border-style:None;">@Model.Request</textarea>

    </div>
    <div class="form-group">
        <label for="Response" class="col-form-label">Response</label>
        <textarea disabled="disabled" class="form-control" style="border-style:None;">@Model.Response</textarea>
    </div>
}
else
{
    <div class="row">
        <h5>Nothing To Show here</h5>
    </div>
}
